  "scripts": {
    "build": "npm run clean && babel src --ignore test/monitor --out-dir dist --source-maps",
    "start": "pm2-runtime start ecosystem.json5 --env $NODE_ENV",
    "local": "node_modules/.bin/pm2 startOrRestart ecosystem.json5 && node_modules/.bin/pm2 logs trips-api --lines=20",
    "test": "mocha --exit --require @babel/register --require ./testHelper.js test/**/*test.js test/**/**/*test.js",
    "stop": "node_modules/.bin/pm2 kill && node_modules/.bin/pm2 flush && node_modules/.bin/pm2 list",
    "restart-local": "npm stop && npm run local",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "migrate_test": "NODE_ENV=test knex migrate:latest",
    "delete-all-tables": "node db/deleteAllTables.js",
    "delete-test-tables": "NODE_ENV=test node db/deleteAllTables.js",
    "eslint": "eslint --ext .js src --fix",
    "rebuild-db": "acl-delete-all-tables && npm run migrate && npm run seed",
    "clean": "rimraf dist"
  }
}
